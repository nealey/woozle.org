/Courier findfont 9 scalefont setfont

/str (  ) def

/points {
    gsave
    translate

    0 1 1 {
        /hunnert exch def
        hunnert 1 eq { 0 -110 translate } if

        0 1 9 {
            /i exch def

            0 i -11 mul moveto
            0 1 9 {
                i 10 mul add /x exch def

                x 10 lt
                    { hunnert x add 0 eq 
                        { (  ) show }
                        { ( ) show 
                          x str cvs show }
                        ifelse } 
                    { x str cvs show }
                    ifelse
                ( ) show
            } for
        } for
    } for
    
    grestore
} def

/phi 18 def
/plr 40 def
/maj 120 def
/min 80 def

/penalties {
    gsave
    translate

    0 0 moveto
    0 plr maj min add add add 0 rlineto
    0 0 moveto

    1 1 16 {
        /i exch def

        0 phi neg rlineto
        plr 0 rlineto
        0 phi rlineto
        0 phi neg rmoveto

        maj 0 rlineto
        0 phi rlineto  0 phi neg rmoveto

        min 0 rlineto
        0 phi rlineto  0 phi neg rmoveto

        plr maj min add add neg 0 rmoveto
    } for

    stroke

    grestore
} def

